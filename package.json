{
  "name": "iidy",
  "version": "1.5.0",
  "description": "A tool for working with CloudFormation and yaml templates.",
  "main": "lib/cli.js",
  "types": "lib/index",
  "bin": "bin/iidy",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "dev-test-watch": "mocha-typescript-watch -p tsconfig.json lib/tests/*js",
    "lint": "tslint -p . src/*ts src/**/*ts",
    "pkg-binaries": "pkg --out-path dist -t node8-macos,node8-linux package.json",
    "test": "make test"
  },
  "author": "Tavis Rudd <tavis@unbounce.com>",
  "repository": "https://github.com/unbounce/iidy",
  "license": "MIT",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "aws-sdk": "^2.104.0",
    "bluebird": "^3.5.0",
    "cli-color": "^1.2.0",
    "dateformat": "^2.0.0",
    "handlebars": "^4.0.10",
    "inquirer": "^3.2.2",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "ntp-client": "^0.5.3",
    "openpgp": "^2.5.8",
    "ora": "^1.3.0",
    "project-name-generator": "^2.1.3",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "tmp": "0.0.31",
    "tv4": "^1.3.0",
    "winston": "^2.3.1",
    "wrap-ansi": "^3.0.1",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/bluebird-global": "^3.5.3",
    "@types/chai": "^4.0.4",
    "@types/cli-color": "^0.3.29",
    "@types/dateformat": "^1.0.1",
    "@types/handlebars": "^4.0.33",
    "@types/inquirer": "0.0.35",
    "@types/js-yaml": "^3.5.31",
    "@types/lodash": "^4.14.68",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.7",
    "@types/ora": "^1.3.0",
    "@types/pad": "^1.0.0",
    "@types/request": "0.0.46",
    "@types/request-promise": "^4.1.35",
    "@types/request-promise-native": "^1.0.6",
    "@types/sprintf-js": "0.0.27",
    "@types/tmp": "0.0.33",
    "@types/tv4": "^1.2.28",
    "@types/winston": "^2.3.3",
    "@types/wrap-ansi": "^2.0.14",
    "@types/yargs": "^8.0.1",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "mocha-typescript": "^1.1.9",
    "pkg": "^4.2.4",
    "tslint": "^5.4.3",
    "typescript": "^2.4.2"
  }
}
